<?xml version="1.0" encoding="UTF-8"?>
<dividends>
  <% for(let i=0;i<data.length;i++) { %>
    <dividend>
      <payoutratio><%= data[i].payoutratio+"%" %></payoutratio>
      <amount><%= data[i].amount.replace(".",",")+"%" %></amount>
      <perstock><%= data[i].perstock.replace(".",",") %></perstock>
      <date><%= data[i].year+"-"+data[i].month+"-"+data[i].day %></date>
    </dividend>
    <% } %>

</dividends>